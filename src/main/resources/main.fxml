<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="280.0" prefWidth="426.0" stylesheets="@style.css" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kanin.cerebrum.Main">
   <children>
      <Button fx:id="connect" layoutX="26.0" layoutY="91.0" mnemonicParsing="false" onAction="#connect" prefHeight="25.0" prefWidth="150.0" stylesheets="@style.css" text="Connect" />
      <Button fx:id="disconnect" layoutX="26.0" layoutY="140.0" mnemonicParsing="false" onAction="#disconnect" prefHeight="25.0" prefWidth="150.0" text="Disconnect" />
      <ProgressIndicator fx:id="status" layoutX="142.0" layoutY="55.0" progress="0.0" />
      <Label fx:id="statusLabel" layoutX="26.0" layoutY="58.0" text="Connection Status" textFill="WHITE" />
      <Button fx:id="settings" layoutX="26.0" layoutY="188.0" mnemonicParsing="false" onAction="#openSettings" prefHeight="25.0" prefWidth="150.0" text="Settings" />
      <ListView fx:id="liveConnections" layoutX="196.0" layoutY="88.0" prefHeight="129.0" prefWidth="202.0" />
      <Label fx:id="liveLabel" layoutX="182.0" layoutY="58.0" text="Live Connections - Click to Disconnect" textFill="WHITE" />
   </children>
</AnchorPane>